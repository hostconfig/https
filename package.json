{
  "name": "@hostconfig/https",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "node ./dist/index.js",
    "start": "yarn prod",
    "restart": "NODE_ENV=production yarn rebuild && yarn serve",
    "build": "tsc -p ./tsconfig.json && tsc-alias && ts-add-js-extension --dir=dist",
    "rebuild": "yarn clean && tsc -p ./tsconfig.json && tsc-alias && ts-add-js-extension --dir=dist",
    "dev": "NODE_ENV=development yarn serve",
    "prod": "NODE_ENV=production yarn serve",
    "clean": "rimraf dist",
    "wipe": "yarn clean && rimraf node_modules",
    "gen:ssl": "./generate.sh"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http-errors": "^2.0.0",
    "https": "^1.0.0",
    "morgan": "^1.10.0",
    "rimraf": "^5.0.5",
    "ts-add-js-extension": "^1.6.0",
    "tsc-alias": "^1.8.8"
  }
}
